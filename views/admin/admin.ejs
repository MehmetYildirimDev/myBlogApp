<%- include ('../partials/header') %>

<!-- Page Header-->
<header class="masthead" style="background-image: url('https://cdn.pixabay.com/photo/2022/06/21/16/18/orange-7276122__340.jpg')">
    <div class="container position-relative px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
            <div class="col-md-10 col-lg-8 col-xl-7">
                <div class="page-heading">
                    <h1>Admin DashBoard</h1>
                </div>
            </div>
        </div>
    </div>
</header>





        <!-- Main Content-->
        <div class="container px-4 px-lg-5">
            <% adminActions.forEach((action) =>{  %>            
            <div class="row gx-4 gx-lg-5 justify-content-center">
                <div class="col-lg-4 mx-auto">
                    <button id="<%=action.actionId %>" onclick="catchID(this.id)" type="button" class="btn btn-primary btn-block float-left"> <%=action.displayName %></button>
                </div>
                <div class="col-lg-8 mx-auto">

                </div>
            </div>
            <% }); %>
        </div>

        <script>
            function catchID(click_id){
                alert(click_id +' numarali buttun');
                if(click_id == 1){
                    window.location.href='changeHomeImage'; return false;
                }
            }
        </script>

<%- include ('../partials/footer') %>